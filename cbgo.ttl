# First version IZ 20230220
# Added DTB-properties IZ 20230613
# Downgraded properties to RDF instead of OWL IZ 20230617

@prefix : <https://data.cbg.nl/ontology#> .

@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .

@prefix prov:   <http://www.w3.org/ns/prov#> .
@prefix pnv:    <https://w3id.org/pnv#> .
@prefix schema: <http://schema.org/> .

# ontology
<https://data.cbg.nl/ontology> a owl:Ontology .

# classes
:Person a owl:Class ;
    rdfs:label "Persoon"@nl ;
    rdfs:subClassOf pnv:Person ;
    rdfs:subClassOf prov:Entity .

:PersonObservation a owl:Class ;
    rdfs:label "Persoonsvermelding"@nl ;
    rdfs:subClassOf :Person .

:PersonReconstruction a owl:Class ;
    rdfs:label "Persoonsreconstructie"@nl ;
    rdfs:subClassOf :Person .

:Document a owl:Class ;
    rdfs:label "Brondocument"@nl ;
    rdfs:subClassOf schema:CreativeWork ;
    rdfs:subClassOf prov:Entity .

# properties
:hasAge a rdf:Property ;
    rdfs:label "heeft leeftijd"@nl ;
    rdfs:domain :PersonObservation .

:hasReligion a rdf:Property ;
    rdfs:label "heeft religie"@nl ;
    rdfs:domain :PersonObservation .

:hasRole a rdf:Property ;
    rdfs:label "heeft rol"@nl ;
    rdfs:domain :PersonObservation .

:baptizationDate a rdf:Property ;
    rdfs:label "is gedoopt op"@nl ;
    rdfs:domain :Person ;
    rdfs:range rdfs:Literal .

:burialDate a rdf:Property ;
    rdfs:label "is begraven op"@nl ;
    rdfs:domain :Person ;
    rdfs:range rdfs:Literal .

:betrothalDate a rdf:Property ;
    rdfs:label "is in ondertrouw gegaan op"@nl ;
    rdfs:domain :Person ;
    rdfs:range rdfs:Literal .

# Properties - Interpersonal relations
:grandparent a rdf:Property ;
    rdfs:label "heeft grootouder"@nl ;
    rdfs:subPropertyOf schema:relatedTo ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:grandchild a rdf:Property ;
    rdfs:label "heeft kleinkind"@nl ;
    rdfs:subPropertyOf schema:relatedTo ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:godparent a rdf:Property ;
    rdfs:label "heeft peetouder"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:godchild a rdf:Property ;
    rdfs:label "heeft petekind"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:previousPartner a rdf:Property ;
    rdfs:label "heeft eerdere partner"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:wid a rdf:Property ;
    rdfs:label "is weduwe/weduwnaar van" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:stepparent a rdf:Property ;
    rdfs:label "heeft stiefouder" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:stepchild a rdf:Property ;
    rdfs:label "heeft stiefkind" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:child-in-law a rdf:Property ;
    rdfs:label "heeft schoonkind" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:parent-in-law a rdf:Property ;
    rdfs:label "heeft schoonouder" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:half-sibbling a rdf:Property ;
    rdfs:label "heeft halbroer/halfzus" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:step-sibbling a rdf:Property ;
    rdfs:label "heeft stiefbroer/stiefzus" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:sibbling-in-law a rdf:Property ;
    rdfs:label "heeft schoonbroer/schoonzus" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:great-grandparent a rdf:Property ;
    rdfs:label "heeft overgrootouder" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:great-grandchild a rdf:Property ;
    rdfs:label "heeft achterkleinkind" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:fosterParent a rdf:Property ;
    rdfs:label "heeft pleegouder" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:fosterChild a rdf:Property ;
    rdfs:label "heeft pleegkind" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:stepparent-in-law a rdf:Property ;
    rdfs:label "heeft stiefschoonouder" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:stepchild-in-law a rdf:Property ;
    rdfs:label "heeft stiefschoonkind" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:grandparent-in-law a rdf:Property ;
    rdfs:label "heeft schoongrootouder" ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:grandchild-in-law a rdf:Property ;
    rdfs:label "heeft schoonkleinkind" ;
    rdfs:domain :Person ;
    rdfs:range :Person .
