                          reconstruction.
                       """@en ;
    rdfs:subPropertyOf sdo:relatedTo ;
    rdfs:domain        picom:Person ;
    rdfs:range         picom:Person ;
    owl:inverseOf      picom:hasStepparent-in-law ;
.

