# Version: 20230714

@prefix : <https://data.cbg.nl/ontology#> .

@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .

@prefix prov:   <http://www.w3.org/ns/prov#> .
@prefix pnv:    <https://w3id.org/pnv#> .
@prefix schema: <http://schema.org/> .

# ontology
<https://data.cbg.nl/ontology> a owl:Ontology .

# Basic classes
:Person a owl:Class ;
    rdfs:label "Persoon"@nl ;
    rdfs:subClassOf pnv:Person ;
    rdfs:subClassOf prov:Entity .

:PersonObservation a owl:Class ;
    rdfs:label "Persoonsvermelding"@nl ;
    rdfs:subClassOf :Person .

:PersonReconstruction a owl:Class ;
    rdfs:label "Persoonsreconstructie"@nl ;
    rdfs:subClassOf :Person .

:Document a owl:Class ;
    rdfs:label "Brondocument"@nl ;
    rdfs:subClassOf schema:CreativeWork ;
    rdfs:subClassOf prov:Entity .

# Event classes
:LifeEvent a owl:Class ;
    rdfs:label "Levensgebeurtenis"@nl .
    rdfs:subClassOf schema:Event .

:Birth a owl:Class ;
    rdfs:label "Geboorte"@nl ;
    rdfs:subClassOf :LifeEvent .

:Baptization a owl:Class ;
    rdfs:label "Doop"@nl ;
    rdfs:subClassOf :LifeEvent .

:Betrothal a owl:Class ;
    rdfs:label "Ondertrouw"@nl ;
    rdfs:subClassOf :LifeEvent .

:Marriage a owl:Class ;
    rdfs:label "Huwelijk"@nl ;
    rdfs:subClassOf :LifeEvent .

:Death a owl:Class ;
    rdfs:label "Overlijden"@nl ;
    rdfs:subClassOf :LifeEvent .

:Burial a owl:Class ;
    rdfs:label "Begrafenis"@nl ;
    rdfs:subClassOf :LifeEvent .

# Properties - Spatio-temporal stuff
:location a rdf:Property ;
    rdfs:label "vond plaats in"@nl .

:date a rdf:Property ;
    rdfs:label "vond plaats op"@nl ;
    rdfs:range rdfs:Literal . # preferably xsd:date, xsd:gYear or the like

:startDate a rdf:Property ;
    rdfs:label "vond plaats van"@nl ;
    rdfs:range rdfs:Literal . # preferably xsd:date, xsd:gYear or the like

:endDate a rdf:Property ;
    rdfs:label "vond plaats tot"@nl ;
    rdfs:range rdfs:Literal . # preferably xsd:date, xsd:gYear or the like

# Properties - observation dependent
:hasAge a rdf:Property ;
    rdfs:label "heeft leeftijd"@nl ;
    rdfs:domain :PersonObservation .

:hasRole a rdf:Property ;
    rdfs:label "heeft rol"@nl ;
    rdfs:domain :PersonObservation .

# Properties - observation INdependent
:hasReligion a rdf:Property ;
    rdfs:label "heeft religie"@nl ;
    rdfs:domain :Person .

:hasLifeEvent a rdf:Property ;
    rdfs:label "heeft levensgebeurtenis"
    rdfs:domain :Person ;
    rdfs:range :LifeEvent .

# Properties - Interpersonal relations
:grandparent a rdf:Property ;
    rdfs:label "heeft grootouder"@nl ;
    rdfs:subPropertyOf schema:relatedTo ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:grandchild a rdf:Property ;
    rdfs:label "heeft kleinkind"@nl ;
    rdfs:subPropertyOf schema:relatedTo ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:godparent a rdf:Property ;
    rdfs:label "heeft peetouder"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:godchild a rdf:Property ;
    rdfs:label "heeft petekind"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:previousPartner a rdf:Property ;
    rdfs:label "heeft eerdere partner"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:wid a rdf:Property ;
    rdfs:label "is weduwe/weduwnaar van"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:stepparent a rdf:Property ;
    rdfs:label "heeft stiefouder"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:stepchild a rdf:Property ;
    rdfs:label "heeft stiefkind"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:child-in-law a rdf:Property ;
    rdfs:label "heeft schoonkind"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:parent-in-law a rdf:Property ;
    rdfs:label "heeft schoonouder"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:half-sibling a rdf:Property ;
    rdfs:label "heeft halfbroer/halfzus"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:step-sibling a rdf:Property ;
    rdfs:label "heeft stiefbroer/stiefzus"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:sibling-in-law a rdf:Property ;
    rdfs:label "heeft zwager/schoonzus"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:great-grandparent a rdf:Property ;
    rdfs:label "heeft overgrootouder"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:great-grandchild a rdf:Property ;
    rdfs:label "heeft achterkleinkind"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:fosterParent a rdf:Property ;
    rdfs:label "heeft pleegouder"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:fosterChild a rdf:Property ;
    rdfs:label "heeft pleegkind"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:stepparent-in-law a rdf:Property ;
    rdfs:label "heeft stiefschoonouder"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:stepchild-in-law a rdf:Property ;
    rdfs:label "heeft stiefschoonkind"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:grandparent-in-law a rdf:Property ;
    rdfs:label "heeft schoongrootouder"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:grandchild-in-law a rdf:Property ;
    rdfs:label "heeft schoonkleinkind"@nl ;
    rdfs:domain :Person ;
    rdfs:range :Person .
