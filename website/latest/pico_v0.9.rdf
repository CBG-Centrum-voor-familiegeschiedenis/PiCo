  <rdf:Description rdf:about="http://www.w3.org/ns/prov#wasGeneratedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov#wasGeneratedBy"/>
    <skos:definition xml:lang="en">Generation is the completion of production of a new Person reconstruction by an activity.
                          This Person reconstruction did not exist before generation and becomes available
                          for usage after this generation.
                       </skos:definition>
    <rdfs:label xml:lang="en">wasGeneratedBy</rdfs:label>
    <skos:scopeNote xml:lang="en">Required for PersonReconstructions.</skos:scopeNote>
    <sdo:domainIncludes rdf:resource="https://personsincontext.org/model/PersonReconstruction"/>
    <sdo:rangeIncludes rdf:resource="http://www.w3.org/ns/prov#Activity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/prov#endedAtTime">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov#endedAtTime"/>
