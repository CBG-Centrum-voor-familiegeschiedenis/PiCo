  <rdf:Description rdf:about="https://schema.org/ImageObject">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://schema.org/CreativeWork"/>
    <skos:definition xml:lang="en">An image file.</skos:definition>
    <rdfs:label xml:lang="en">Scan</rdfs:label>
    <skos:scopeNote xml:lang="en">A link to the location of the scan at the HoldingArchive.
                        A link to scan as it can be displayed online and a link to a thumbnail (for search results)
                        should be provided. Preferably a link to a IIIF manifest.
                       </skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="https://personsincontext.org/model/hasRole">
