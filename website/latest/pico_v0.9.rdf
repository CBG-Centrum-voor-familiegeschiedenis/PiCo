  <rdf:Description rdf:about="http://www.w3.org/ns/prov#wasDerivedFrom">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
    <skos:definition xml:lang="en">A derivation is a combination of one or more Person observations
                             in a Person reconstruction</skos:definition>
    <rdfs:label xml:lang="en">wasDerivedFrom</rdfs:label>
    <skos:scopeNote xml:lang="en">A PersonReconstruction is always derived from one or more PersonObservations.
                             The wasDerivedFrom property is therefore required for PersonReconstructions.
                          </skos:scopeNote>
    <sdo:domainIncludes rdf:resource="https://personsincontext.org/model/PersonReconstruction"/>
    <sdo:rangeIncludes rdf:resource="https://personsincontext.org/model/PersonObservation"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://schema.org/url">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://schema.org/url"/>
    <skos:definition>URL of the Source or Scan.</skos:definition>
    <rdfs:label xml:lang="en">url</rdfs:label>
    <skos:scopeNote xml:lang="en">For Sources: a link to the Source at the holding archive. Preferably a permalink.</skos:scopeNote>
    <skos:scopeNote xml:lang="en">For Scans of Sources: a link to the scan of the Source at the holding archive.
                          Preferably a link to a IIIF manifest.
                       </skos:scopeNote>
    <sdo:domainIncludes rdf:resource="https://schema.org/ImageObject"/>
    <sdo:domainIncludes rdf:resource="https://schema.org/ArchiveComponent"/>
    <sdo:rangeIncludes rdf:resource="https://schema.org/URL"/>
  </rdf:Description>
