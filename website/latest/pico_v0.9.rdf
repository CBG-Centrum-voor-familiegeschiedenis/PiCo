  <rdf:Description rdf:about="https://schema.org/name">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://schema.org/name"/>
    <skos:definition>The name of a Person or a Source.</skos:definition>
    <rdfs:label xml:lang="en">name</rdfs:label>
    <skos:scopeNote xml:lang="en">For Persons: Use full name if the name can't be split into given name and family name.
                          Otherwise use sdo:givenNamen and sdo:fullName or the more extensive pnv:Name.
                       </skos:scopeNote>
    <skos:scopeNote xml:lang="en">For Sources: Use a user-friendly name to identify the source. Depending on the Source type,
                          this may contain a combination of it's name, year of creation, creator, et cetera.
                       </skos:scopeNote>
    <sdo:domainIncludes rdf:resource="https://personsincontext.org/model/Person"/>
    <sdo:domainIncludes rdf:resource="https://schema.org/ArchiveComponent"/>
    <sdo:rangeIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
