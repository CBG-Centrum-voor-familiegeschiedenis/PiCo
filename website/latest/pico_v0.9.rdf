  <rdf:Description rdf:about="https://w3id.org/pnv#patronym">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/pnv#patronym"/>
    <skos:definition>A Patronym is a name element based on the given name of one's father or grandfather.
                             This is also the preferred property for a matronym (i.e. a name element based on the given
                             name of one's mother or grandmother).
                          </skos:definition>
    <rdfs:label xml:lang="en">patronym</rdfs:label>
    <sdo:domainIncludes rdf:resource="https://w3id.org/pnv#Name"/>
    <sdo:rangeIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
