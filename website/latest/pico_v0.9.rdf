    <rdfs:isDefinedBy rdf:resource="https://schema.org/deathPlace"/>
    <skos:definition xml:lang="en">The place where the person died.</skos:definition>
    <skos:scopeNote xml:lang="en">Preferably, use a link to an existing thesaurus like geonames or wikidata.
                          If desired, both the literal place, as mentioned on the Source can be used together with
                          the reconciled version.
                       </skos:scopeNote>
    <rdfs:label xml:lang="en">deathPlace</rdfs:label>
