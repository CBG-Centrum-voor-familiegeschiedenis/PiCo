    <rdfs:label>startedAtTime</rdfs:label>
    <sdo:domainIncludes rdf:resource="http://www.w3.org/ns/prov#Activity"/>
    <sdo:rangeIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://schema.org/spouse">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://schema.org/spouse"/>
    <skos:definition xml:lang="en">The person's spouse.</skos:definition>
    <rdfs:label xml:lang="en">spouse</rdfs:label>
    <skos:scopeNote xml:lang="en">For Person observations, the spouse should always refer to another Person observation
                             on the same Source.
                          </skos:scopeNote>
    <skos:scopeNote xml:lang="en">For Person reconstructions, the spouse should refer to the Person reconstruction of
                             the spouse.
                          </skos:scopeNote>
    <rdfs:subPropertyOf rdf:resource="https://schema.org/relatedTo"/>
    <sdo:domainIncludes rdf:resource="https://personsincontext.org/model/Person"/>
    <sdo:rangeIncludes rdf:resource="https://personsincontext.org/model/Person"/>
