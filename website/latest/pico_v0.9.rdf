  <rdf:Description rdf:about="https://schema.org/birthDate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://schema.org/birthDate"/>
    <skos:definition>The birth date of a persoon.</skos:definition>
    <rdfs:label xml:lang="en">birthDate</rdfs:label>
    <skos:scopeNote xml:lang="en">The birth date of a person. A date value in ISO 8601 format should be used.
                          Incomplete dates are allowed, but only when truncated from small to large. For example:
                          "1970-08-15", "1970-08" and "1970" are all allowed, but "1970-00-15" or "08-15" are not.
                          Sometimes, you may want use a string as birthDate. For example if you want to or have to
                          record the date exactly as it was written on the Source. If possible use both the string
                          format and the xsd:date format in this case.
                       </skos:scopeNote>
    <sdo:domainIncludes rdf:resource="https://personsincontext.org/model/Person"/>
    <sdo:rangeIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <sdo:rangeIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
