    <rdfs:label xml:lang="en">hasRole</rdfs:label>
    <skos:scopeNote xml:lang="en">Use a term from the PiCo Roles thesaurus:
                          https://terms.personsincontext.org/roles
                       </skos:scopeNote>
    <sdo:domainIncludes rdf:resource="https://personsincontext.org/model/PersonObservation"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://personsincontext.org/model/hasAncestor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:label xml:lang="en">hasAncestor</rdfs:label>
    <skos:scopeNote xml:lang="en">hasAncestor is not meant to be used as a property for PersonObservations or
                          PersonReconstructions.
                          It is a superclass of hasParent. Since hasAncestor is a owl:TransitiveProperty, this will make
                          the parent of your parent into your ancestor.
                       </skos:scopeNote>
    <rdfs:subPropertyOf rdf:resource="https://schema.org/relatedTo"/>
    <owl:inverseOf rdf:resource="https://personsincontext.org/model/hasDescendant"/>
    <rdfs:domain rdf:resource="https://personsincontext.org/model/Person"/>
    <rdfs:range rdf:resource="https://personsincontext.org/model/Person"/>
  </rdf:Description>
