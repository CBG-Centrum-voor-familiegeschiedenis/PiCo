  <rdf:Description rdf:about="https://personsincontext.org/model/hasAge">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <skos:definition xml:lang="en">The age of a person.</skos:definition>
    <rdfs:label xml:lang="en">hasAge</rdfs:label>
    <skos:scopeNote xml:lang="en">Only used for Person observations, in case the birth date is not known,
                                  but the age is. In combination with de date of the Source, a birth date range can
                                  be derived, which can be used in the process of creating a Person reconstruction.
                                  The age is considered to be in years, unless otherwise mentioned. So an age of
                                  "4" is considered to be 4 years, where "4 months" is interpreted as 4 months.
                                  Numerical ages ("4") are preferred above ages in text ("four").
                               </skos:scopeNote>
    <sdo:domainIncludes rdf:resource="https://personsincontext.org/model/PersonObservation"/>
    <sdo:rangeIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
